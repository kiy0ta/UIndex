<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/date_back_color"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/first_date"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@color/first_color"
            android:gravity="center_vertical"
            android:paddingLeft="4dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

        <TextView
            android:id="@+id/first_practice"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/first_color"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/second_date"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@color/second_color"
            android:gravity="center_vertical"
            android:paddingLeft="4dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

        <TextView
            android:id="@+id/second_practice"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/second_color"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/third_date"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@color/third_color"
            android:gravity="center_vertical"
            android:paddingLeft="4dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

        <TextView
            android:id="@+id/third_practice"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/third_color"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/forth_date"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@color/forth_color"
            android:gravity="center_vertical"
            android:paddingLeft="4dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

        <TextView
            android:id="@+id/forth_practice"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/forth_color"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/fifth_date"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@color/fifth_color"
            android:gravity="center_vertical"
            android:paddingLeft="4dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

        <TextView
            android:id="@+id/fifth_practice"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/date_text_color"
            android:textSize="20dp"></TextView>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/fifth_color"></View>


</LinearLayout>
